  <class_name>CallMetaMeasureEnergyPlus</class_name>
  <display_name>Call Meta Measure for EnergyPlus Measures</display_name>
  <description>Measure that calls a child measure based on the sample value and probability distribution file provided.</description>
  <modeler_description>Based on the sample value provided by the sampling algorithm and the housing characteristics probability distribution file, a child measure will be called with appropriate arguments. This measure also handles any upstream dependencies that have been previously set.</modeler_description>
  <arguments>
    <argument>
      <name>probability_file</name>
      <display_name>Probability DistributionFile.tsv</display_name>
      <description>The name of the file that provides probability distributions. The file's directory is currently hard-coded.</description>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>sample_value</name>
      <display_name>Sample Value</display_name>
      <description>The sample value determined by the OpenStudio sampling algorithm.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
