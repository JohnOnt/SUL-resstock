        "option_1_apply_logic": "(HVAC Cooling Type|None)||(HVAC Cooling Type|Central AC)||(HVAC Cooling Type|Room AC)",
        "option_1_lifetime": 20,
        "option_1_cost_1_value": 2000.0,
        "option_1_cost_1_multiplier": "Fixed (1)",
        "option_1_cost_2_value": 80.0,
        "option_1_cost_2_multiplier": "Size, Cooling System (kBtu/h)"
      }
    },
    {
      "measure_dir_name": "SimulationOutputReport"
    },
    {
      "measure_dir_name": "UpgradeCosts"
    }
  ],
  "run_options": {
    "fast": false,
    "skip_expand_objects": true,
    "skip_energyplus_preprocess": true
  }
