      "arguments": {
        "building_id": "1",
        "workflow_json": "measure-info.json"
      }
    },
    {
      "measure_dir_name": "ApplyUpgrade",
      "arguments": {
        "run_measure": 1,
        "upgrade_name": "High Efficiency Central Air Conditioner",
